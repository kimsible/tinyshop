{
  "name": "tinyshop",
  "author": "kimsible",
  "license": "AGPL-3.0",
  "private": true,
  "repository": "github:kimsible/tinyshop",
  "engines": {
    "node": ">=13.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ava"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "standard --fix",
      "./test-staged.sh"
    ]
  },
  "scripts": {
    "postinstall": "mkdir -p ./public; touch ./public/settings.json; touch ./public/products.json",
    "build": "next build",
    "start": "NODE_ENV=production node server"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.2",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "next": "^9.1.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
